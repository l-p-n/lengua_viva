<h2 style="margin-bottom: 42px;">My Notes</h2>
<% @notes.each do |note| %>
  <div class="d-flex" style="margin-bottom: 42px;">

    <%= render note.resource %>

    <div class="review-content mx-5">
      <% note.rating.times do  %>
        <i class="fas fa-star"></i>
      <% end %>
      <p class="mt-2"><%= note.content %></p>
    </div>
  </div>
<% end %>
